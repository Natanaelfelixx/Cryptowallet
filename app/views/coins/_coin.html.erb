<div id="<%= dom_id coin %>">
  <p>
    <strong>Description:</strong>
    <%= coin.description %>
  </p>

  <p>
    <strong>Acronym:</strong>
    <%= coin.acronym %>
  </p>

  <p>
    <strong>Url image:</strong>
    <tr>
      <td><td><%= image_tag coin.url_image, size: "25x25" %></td></td>
    </tr>
  </p>

</div>
